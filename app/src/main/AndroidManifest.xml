<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.activityrecyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ActivityRecyclerViewFragment">
        <activity android:name="com.example.activityrecyclerview.uis.ConfirmChangePasswordActivity"></activity>
        <activity android:name="com.example.activityrecyclerview.uis.HomeActivity" />
        <activity android:name="com.example.activityrecyclerview.uis.EmailActivity" />
        <activity android:name="com.example.activityrecyclerview.uis.SignUpActivity" />
        <activity android:name="com.example.activityrecyclerview.uis.LoginActivity" />
        <activity
            android:name=".uis.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name="com.example.activityrecyclerview.OnBoarding3Activity" />
        <activity android:name="com.example.activityrecyclerview.OnBoarding2Activity" />
        <activity android:name="com.example.activityrecyclerview.uis.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>